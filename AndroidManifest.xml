<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.shanzid.wordPlay">
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.shanzid.wordPlay.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_unscramble" android:name="com.shanzid.wordPlay.Unscramble" android:windowSoftInputMode="adjustPan"/>
        <activity android:label="@string/title_activity_scramble" android:name="com.shanzid.wordPlay.Scramble" android:windowSoftInputMode="adjustPan"/>
        <activity android:label="@string/title_activity_game_unsramble" android:name="com.shanzid.wordPlay.Game_unsramble"/>
        <activity android:label="@string/title_activity_game__spelling" android:name="com.shanzid.wordPlay.Game_Spelling"/>
    </application>
</manifest>